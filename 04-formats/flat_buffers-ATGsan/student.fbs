namespace decentralized.atgsan;
 
struct Project {
   repo:[ubyte:59];
   mark:uint8;
}
 
struct Student {
  // имя может быть и короче 32 байт, тогда в хвосте 000
  // имя - валидный UTF-8
  name:[ubyte:32];
  login:[ubyte:16];
  group:[ubyte:8];
  // 0/1, фактически bool
  practice:[uint8:8];
  project:Project;
  // 32 bit IEEE 754 float
  mark:float32;
}

table StudentMessage {
    studentimpl:[Student];
}
root_type StudentMessage;
